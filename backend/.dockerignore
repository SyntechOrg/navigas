# Create .dockerignore file
@"
node_modules
data
.tmp
.cache
.git
build
.env
.strapi
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.DS_Store
"@ | Out-File -FilePath .dockerignore -Encoding utf8

# Now rebuild
docker-compose build --no-cache

# Start
docker-compose up -d
docker-compose logs -f strapi
